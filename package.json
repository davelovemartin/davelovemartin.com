    "validate": "yarn lint && yarn format",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.30.7",
    "astro": "^1.0.0-beta.73",
    "eslint": "^8.20.0",
    "eslint-plugin-astro": "^0.15.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "husky": "^8.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.1.2"
  }
}
